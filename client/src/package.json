{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "yarn": "1.17.3",
    "node": "12.6.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --open --config ../../config/webpack.dev.js",
    "build": "./node_modules/.bin/webpack --config ../../config/webpack.prod.js",
    "lint": "./node_modules/.bin/eslint ./",
    "test": "./node_modules/.bin/jest"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest/globals": true
    },
    "extends": [
      "prettier",
      "airbnb"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest"
    ],
    "rules": {
      "arrow-parens": 0,
      "object-curly-newline": 0,
      "function-paren-newline": 0,
      "import/no-extraneous-dependencies": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0,
      "react/require-default-props": 0,
      "implicit-arrow-linebreak": 0,
      "operator-linebreak": 0,
      "no-param-reassign": 0,
      "jsx-a11y/label-has-for": 0
    },
    "parser": "babel-eslint"
  },
  "eslintIgnore": [
    "dist/**"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "basscss": "^8.1.0",
    "basscss-background-colors": "^2.1.0",
    "basscss-colors": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^9.2.1",
    "postcss": "^7.0.17",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "react-test-renderer": "^16.8.6",
    "scroll-behavior-polyfill": "^2.0.13",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  }
}
